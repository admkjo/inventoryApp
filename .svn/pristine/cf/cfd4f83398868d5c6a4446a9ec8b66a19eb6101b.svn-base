/*InputText*/
input, select,textarea{font-size: 14px !important;}
input:focus,textarea:focus,select:focus{outline:0;color:#333;border-color:#56b4ef !important;-webkit-box-shadow:inset 0 1px 3px rgba(0,0,0,.05),0 0 8px rgba(82,168,236,.6) !important;-moz-box-shadow:inset 0 1px 3px rgba(0,0,0,.05),0 0 8px rgba(82,168,236,.6) !important;box-shadow:inset 0 1px 3px rgba(0,0,0,.05),0 0 8px rgba(82,168,236,.6)!important}
body,label,input,textarea,select,button{font-family:Geneva, Verdana, Arial, Geneva,sans-serif !important}


body.ms-windows,body.ms-windows label,body.ms-windows input,body.ms-windows textarea,body.ms-windows select,body.ms-windows button{font-family:Arial,sans-serif}
body.ar,body.ur,body.fa,body.ar label,body.ar input,body.ar textarea,body.ar select,body.ar button,body.fa label,body.fa input,body.fa textarea,body.fa select,body.fa button,body.ur label,body.ur input,body.ur textarea,body.ur select,body.ur button{font-family:Tahoma,Arial,sans-serif}body.ja,body.ja label,body.ja input,body.ja textarea,body.ja select,body.ja button,body.ja .bio{font-family:"Helvetica Neue",Arial,"Hiragino Kaku Gothic Pro",Meiryo,"MS PGothic",sans-serif;font-style:normal}

input[type=text],input[type=password]{display:inline-block; width: auto; padding:4px;margin: 0px 0px 3px 2px;outline:0;background-color:#fff;border:1px solid #ccc;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px}
select{display:inline-block; width: auto; padding:4px;margin: 0px 0px 3px 2px;outline:0;background-color:#fff;border:1px solid #ccc;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px}
textarea{display:inline-block; min-height: 70px; max-height: 70px; min-width: 300px; max-width: 300px; padding:4px;margin:0;outline:0;background-color:#fff;border:1px solid #ccc;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px}

.ie9 input,.ie9 textarea{min-height:20px}
input[type=checkbox],input[type=radio]{width:auto;height:auto;padding:0;margin:3px 0;*margin-top:0;line-height:normal;border:0;background-color:transparent;cursor:pointer}
input[type=file]{background-color:#fff;padding:initial;border:initial;line-height:initial;-webkit-box-shadow:none;-moz-box-shadow:none;box-shadow:none}
input[type=button],input[type=reset],input[type=submit]{width:auto;height:auto; }

.success{background:#FFF6BF;border:#FFB324;color:#514721;}
.fail,.warn .error, .error_txt{background:#FBE3E4;border:#FBC2C4;color:#8A1F11;}
.info, .invalid{background:#E6EFC2;border:#C6D880;color:#264409;}
ul > .errortxt{
    border: solid 1px #900;
    background:#FFE0E0
}
ul > .infotxt{
    border: solid 1px #99cc00;
    background-color: #eef8d0;
}
ul li.errortxt{background:#FFE0E0}
ul li.infotxt,ul li.errortxt{padding: 0px 20px;line-height: 25px;}
.infotxt{color: #00c200}
.errortxt{color: #8A1F11}


.ui-menu.ui-megamenu .ui-menu-child .ui-menu-list {
    width: 200px!important;
}


