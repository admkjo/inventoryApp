#nav {padding:0; margin:0; list-style:none; height:35px; background:#fff url(../navs/three_0.gif) repeat-x; position:relative; z-index:500; font-family:verdana, arial, sans-serif;}
#nav li.top {display:block; float:left; height:35px; width: auto}
#nav li a.top_link {display:block; float:left; height:35px; line-height:33px; color: #ffe043; text-decoration:none; font-size:12px; font-weight:normal; padding:0 0 0 12px; cursor:pointer;background: url(../navs/three_0.gif);}
#nav li a.top_link span {float:left; display:block; padding:0 24px 0 12px; height:35px; background: url(../navs/three_0.gif) right top no-repeat;}
#nav li a.top_link span.down {float:left; display:block; padding:0 23px 0 2px; height:35px; background: url(../navs/three_0a.gif) no-repeat right top;}
@font-face
{
    font-family: 'ebrima';
    src: url('../font/ebrima.ttf'),
        url('calibril.eof'); /* IE9+ */
}
#nav li:hover a.top_link {color:#fff; background: url(../navs/three_1.gif) no-repeat;}
#nav li:hover a.top_link span {background:url(../navs/three_1.gif) no-repeat right top;}
#nav li:hover a.top_link span.down {background:url(../navs/three_1a.gif) no-repeat right top;}

/* Default list styling */

#nav li:hover {position:relative; z-index:200;}

#nav li:hover ul.sub{left:1px; top:35px; background: #bbd37e; padding:3px; border:1px solid #5c731e; white-space:nowrap; width:auto; height:auto; z-index:300;}
#nav li:hover ul.sub li{display:block; height:20px; position:relative; float:left; width:150px; font-weight:normal; padding:0px 10px 3px 0px}
#nav li:hover ul.sub li a{display:block; font-size:12px; height:18px; width:auto; line-height:18px; text-indent:5px; color:#000; text-decoration:none;border:1px solid #bbd37e;}
#nav li ul.sub li a.fly{background:#bbd37e url(../navs/arrow.gif) 80px 6px no-repeat;}
#nav li:hover ul.sub li a:hover {background:#6a812c; color:#fff; border-color:#fff; padding: 2px; width: 150px}
#nav li:hover ul.sub li a.fly:hover{background:#6a812c url(../navs/arrow_over.gif) 80px 6px no-repeat; color:#fff;}


#nav li:hover li:hover ul,
#nav li:hover li:hover li:hover ul,
#nav li:hover li:hover li:hover li:hover ul,
#nav li:hover li:hover li:hover li:hover li:hover ul
{left:90px; top:-4px; background: #bbd37e; padding:3px; border:1px solid #5c731e; white-space:nowrap; width:90px; z-index:400; height:auto;}

#nav ul, 
#nav li:hover ul ul,
#nav li:hover li:hover ul ul,
#nav li:hover li:hover li:hover ul ul,
#nav li:hover li:hover li:hover li:hover ul ul
{position:absolute; left:-9999px; top:-9999px; width:0; height:0; margin:0; padding:0; list-style:none;}

#nav li:hover li:hover a.fly,
#nav li:hover li:hover li:hover a.fly,
#nav li:hover li:hover li:hover li:hover a.fly,
#nav li:hover li:hover li:hover li:hover li:hover a.fly
{background:#6a812c url(../navs/arrow_over.gif) 80px 6px no-repeat; color:#fff; border-color:#fff;} 

#nav li:hover li:hover li a.fly,
#nav li:hover li:hover li:hover li a.fly,
#nav li:hover li:hover li:hover li:hover li a.fly
{background:#bbd37e url(../navs/arrow.gif) 80px 6px no-repeat; color:#000; border-color:#bbd37e;} 



